@model IEnumerable<MyUser>;

@inject UserManager<MyUser> userManager
<a class="btn btn-dark">Create</a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">User</th>
            <th scope="col">Email</th>
            <th scope="col">Role</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            var role = await userManager.GetRolesAsync(item);
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                <td>@role[0]</td>
                <td>
                    <a class="btn btn-outline-dark">Edit</a>
                    <a class="btn btn-dark" onclick="return confirm('Are you sure?')">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>